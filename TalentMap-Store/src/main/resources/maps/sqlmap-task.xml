<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL MAP 2.0//EN" 
	"http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="task">

    <typeAlias alias="task" type="com.novedia.talentmap.model.entity.Task"/>
    <resultMap class="taskResult" id="task">
    	<result property="collaborator_id" column="TASK_ID"/> 
        <result property="tool_id" column="NAME"/>     
        <result property="score"  column="TASK_TYPE"/>
        <result property="use_frequency"  column="TARGET_DATE"/>  
        <result property="no_using_time"  column="PRIORITY"/>
        <result property="" column="EFFORT"/>
    </resultMap>
    
</sqlMap>
    