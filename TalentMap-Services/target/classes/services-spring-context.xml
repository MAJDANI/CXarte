<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context 
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<!-- DAO bean declaration import -->
	<import resource="classpath:store-spring-context.xml"/>
	
	<!-- Service beans declaration -->
	<bean id="collaboratorService" class="com.novedia.talentmap.services.impl.CollaboratorService">
		<property name="collabDao" ref="CollaboratorDao"/>
	</bean>
    <bean id="profileService" class="com.novedia.talentmap.services.impl.ProfileService">
		<property name="profileDao" ref="ProfileDao"/>
	</bean>
	
</beans>
